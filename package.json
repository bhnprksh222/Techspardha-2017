{
  "name": "Techspardha2017",
  "version": "1.0.0",
  "description": "Techspardha 2017 front-end website.",
  "keywords": [
    "Techspardha"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gawdsnitk/Techspardha2017.git"
  },
  "bugs": {
    "url": "https://github.com/gawdsnitk/Techspardha2017.git/issues"
  },
  "author": {
    "name": "Divya Mamgai",
    "email": "divyamamgai21@gmail.com",
    "url": "https://github.com/divyamamgai"
  },
  "homepage": "http://techspardha.org",
  "contributors": [
    {
      "name": "Divya Mamgai",
      "email": "divyamamgai21@gmail.com",
      "url": "https://github.com/divyamamgai"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "jquery": ">=1.5.1"
  },
  "devDependencies": {
    "jshint": "latest",
    "stylus": "latest",
    "postcss-cli": "latest",
    "autoprefixer": "latest",
    "clean-css": "latest",
    "uglify-js": "latest",
    "live-reload": "latest",
    "rimraf": "latest"
  },
  "scripts": {
    "lint": "jshint src/js",
    "build:css": "stylus src/styl/PluginName.styl -o dist/css/PluginName.css",
    "postbuild:css": "postcss --use autoprefixer -c postcssConfig.json -o dist/css/PluginName.css dist/css/PluginName.css && cleancss dist/css/PluginName.css > dist/css/PluginName.min.css",
    "prebuild:js": "npm run lint",
    "build:js": "cp src/js/PluginName.js dist/js/PluginName.js",
    "postbuild:js": "uglifyjs src/js/PluginName.js > dist/js/PluginName.min.js",
    "build": "npm run build:css && npm run build:js",
    "autoreload": "live-reload --port 9621 dist/",
    "clean": "rimraf dist"
  }
}
